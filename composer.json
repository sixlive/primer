{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "adamwathan/laravel-preset": "@dev",
        "fideloper/proxy": "~3.3",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "sentry/sentry-laravel": "^0.8.0"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "friendsofphp/php-cs-fixer": "^2.8",
        "fzaninotto/faker": "~1.4",
        "laravel-frontend-presets/tailwindcss": "^0.2.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "sensiolabs/security-checker": "^4.1"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "styles:check": "vendor/bin/php-cs-fixer fix --dry-run --diff",
        "styles:fix": "vendor/bin/php-cs-fixer fix"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/adamwathan/laravel-preset"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
